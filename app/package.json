{
  "name": "minesweeper-api",
  "version": "1.0.0",
  "description": "API made with wolkenkit.",
  "wolkenkit": {
    "application": "minesweeper",
    "runtime": {
      "version": "3.1.0"
    },
    "environments": {
      "default": {
        "api": {
          "address": {
            "host": "local.wolkenkit.io",
            "port": 3000
          },
          "allowAccessFrom": "*"
        },
        "fileStorage": {
          "allowAccessFrom": "*"
        },
        "identityProvider": {
          "name": "https://jacortesmahmud.auth0.com/",
          "certificate": "/server/keys/auth0"
        },
        "node": {
          "environment": "development"
        }
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/thenativeweb/wolkenkit-template-empty.git"
  },
  "keywords": [
    "wolkenkit"
  ],
  "license": "AGPL-3.0",
  "dependencies": {
    "wolkenkit-command-tools": "^3.0.0"
  }
}
